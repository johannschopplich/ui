{
  "name": "@byjohann/ui",
  "type": "module",
  "version": "0.12.2",
  "packageManager": "pnpm@10.7.1",
  "description": "A collection of Vue.js components for my projects",
  "author": "Johann Schopplich <hello@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/ui.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/ui/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt/index.d.ts",
      "default": "./dist/nuxt/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "components.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "test:types": "vue-tsc --noEmit",
    "release": "bumpp"
  },
  "peerDependencies": {
    "@vueuse/core": "*",
    "csstype": "^3",
    "vue": "^3"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.2",
    "@nuxt/schema": "^3.16.2"
  },
  "optionalDependencies": {
    "medium-zoom": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@iconify-json/ph": "^1.2.2",
    "@types/node": "^22.13.17",
    "@unocss/eslint-config": "^66.1.0-beta.8",
    "@unocss/reset": "^66.1.0-beta.8",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vueuse/core": "^13.0.0",
    "bumpp": "^10.1.0",
    "csstype": "^3.1.3",
    "eslint": "^9.23.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "unocss": "^66.1.0-beta.8",
    "vite": "^6.2.4",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  }
}
